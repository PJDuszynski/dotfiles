@theme "/usr/share/rofi/themes/solarized_alternate.rasi"

configuration {
    modes: "window,drun,run,ssh";
    font: "Hack Nert Font 14";
    show-icons: true;


  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }

}